<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <LangVersion>9</LangVersion>
        <Authors>Ellersoft LLC</Authors>
        <Description>This project is a wrapper over the Microsoft SignalR implementation which simplifies the process of using SignalR.</Description>
        <Copyright>Copyright (c) 2021 Ellersoft LLC</Copyright>
        <PackageProjectUrl>https://github.com/ellersoft/Ellersoft.SignalR</PackageProjectUrl>
        <RepositoryUrl>https://github.com/ellersoft/Ellersoft.SignalR</RepositoryUrl>
        <Company>Ellersoft LLC</Company>
        <AssemblyVersion>1.0.0.0</AssemblyVersion>
        <FileVersion>1.0.0.0</FileVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="3.1.11" />
      <PackageReference Include="Microsoft.AspNetCore.SignalR" Version="1.1.0" />
      <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.8.0" />
    </ItemGroup>

</Project>
